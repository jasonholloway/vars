# out: firstName age

echo firstName=bob
echo age=70

#++++++++++++++++++++++++
# out: dog
echo dog=¦Bert¦Rex¦Biter¦Nobby



#+++++++++++++++++++++++
# n: sayHello
# in: dog
echo Hello, ${dog}!




#+++++++++++++++++++
# n: sayMoo
# in: cowName
echo "${cowName}: Moo!"



#+++++++++++++++++++
# n: tryTTY
# in: dog age
@tty 'echo DOG1 $dog $age $(tty) >&2' 
@tty 'echo DOG2 $dog $age $(tty) >&2' 
@tty 'echo DOG3 $(@curl google.com) >&2' 

