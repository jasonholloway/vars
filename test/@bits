# out: firstName age
# cache

echo firstName=bob
echo age=70

@cacheTill 999999999999999999999999

#++++++++++++++++++++++++
# out: dog
echo dog=¦Bert¦Rex¦Biter¦Nobby



#+++++++++++++++++++++++
# n: sayHello
# in: dog
echo Hello, ${dog}!




#+++++++++++++++++++
# n: sayMoo
# in: cowName
echo "${cowName}: Moo!"



#+++++++++++++++++++
# n: tty1
# in: dog age
@tty 'echo DOG1 $dog $age $(tty)' 
@tty 'echo DOG2 $dog $age $(tty) >&2' 
@tty 'echo DOG3 $(@curl google.com) >&2' 


#+++++++++++++++++++
# n: tty_noContext
echo hello >&2
@tty 'echo HELLO! $(tty) >&2' 
