# out: firstName age

echo firstName=bob
echo age=70

#++++++++++++++++++++++++
# out: dog
echo dog=¦Bert¦Rex¦Biter¦Nobby



#+++++++++++++++++++++++
# n: sayHello
# in: dog
echo Hello, ${dog}!




#+++++++++++++++++++
# n: sayMoo
# in: cowName
echo "${cowName}: Moo!"



#+++++++++++++++++++
# n: tryTTY
# in: dog
@tty echo $dog

