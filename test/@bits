# out: firstName age
echo firstName=bob$(date +%s)
echo age=70

#++++++++++++++++++++++++
# out: dog
echo dog=¦Bert¦Rex¦Biter¦Nobby



#++++++++++++++++++++ map
dog > food
:	Bert > bones
:	Rex > biscuits
:	* > meat



#+++++++++++++++++++++++
# n: sayHello
# in: dog
echo Hello, ${dog}!


#+++++++++++++++++++++++
# n: greet
# in: firstName
echo yo, $firstName


#+++++++++++++++++++
# n: sayMoo
# in: cowName
echo "${cowName}: Moo!"



#+++++++++++++++++++
# n: tty1
# in: dog age
@tty 'echo DOG1 $dog $age $(tty)' 
@tty 'echo DOG2 $dog $age $(tty) >&2' 
@tty 'echo DOG3 $(@curl google.com) >&2' 


#+++++++++++++++++++
# n: tty_noContext
echo hello >&2
@tty 'echo HELLO! $(tty) >&2' 
